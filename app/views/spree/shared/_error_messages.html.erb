<% if target && target.errors.any? %>
  <div id="errorExplanation" class="alert alert-error" data-hook>
    <p><%= Spree.t(:there_were_problems_with_the_following_fields) %>:</p>
     <ul>
     <% target.errors.full_messages.each do |msg| %>
       <li><%= msg %></li>
     <% end %>
     </ul>
  </div>
<% end %>
